#include <stdio.h>

double miejsceZerowe(int a, int b){
    double temp1 =0;
    temp1 = -b/a;
    return temp1;
}



int main(int argc, const char * argv[]) {
    int a,b; // y = ax + b
    scanf("%d%d",&a,&b);
    printf("%f",miejsceZerowe(a,b));
    return 0;
}
